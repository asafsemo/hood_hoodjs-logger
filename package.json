{
    "name": "@hood/hoodjs-logger",
    "version": "2.0.4",
    "description": "Hood trace logger library",
    "main": "src/index.js",
    "dependencies": {},
    "devDependencies": {
        "tape": "^4.13.2",
        "tap-summary": "^4.0.0"
    },
    "scripts": {
        "reset": "rimraf ./node_modules",
        "test": "npm i && node node_modules/tape/bin/tape ./test/**/*.spec.js",
        "summary": "npm i && node $NODE_DEBUG_OPTION node_modules/tape/bin/tape ./test/**/*.spec.js | tap-summary"
    },
    "directories": {
        "test": "test"
    },
